@article{bernot_structure_2008,
  title = {The Structure of Branched Transportation Networks},
  author = {Bernot, Marc and Caselles, Vicent and Morel, Jean-Michel Michel},
  year = {2008},
  month = nov,
  journal = {Calculus of Variations and Partial Differential Equations},
  volume = {32},
  number = {3},
  pages = {279--317},
  issn = {09442669},
  doi = {10.1007/s00526-007-0139-0},
  abstract = {The transportation problem can be formalized as the problem of finding\$\textbackslash backslash\$nthe optimal paths to transport a measure mu (+) onto a measure mu(-)\$\textbackslash backslash\$nwith the same mass. In contrast with the Monge-Kantorovich\$\textbackslash backslash\$nformalization, recent approaches model the branched structure of such\$\textbackslash backslash\$nsupply networks by an energy functional whose essential feature is to\$\textbackslash backslash\$nfavor wide roads. Given a flow s in a road or a tube or a wire, the\$\textbackslash backslash\$ntransportation cost per unit length is supposed to be proportional to s\$\textbackslash backslash\$n(alpha) with 0 {$<$} alpha {$<$} 1. For the Monge-Kantorovich energy alpha= 1 so\$\textbackslash backslash\$nthat it is equivalent to have two roads with flow 1/2 or a larger one\$\textbackslash backslash\$nwith flow 1. If instead 0 {$<$} alpha {$<$} 1, a road with flow s(1)+s(2) is\$\textbackslash backslash\$npreferable to two individual roads s (1) and s (2) because (s(1)+s(2) )\$\textbackslash backslash\$n{$<$} s(1)(alpha)+s(2)(alpha) . Thus, this very simple model intuitively\$\textbackslash backslash\$nleads to branched transportation structures. Such a branched structure\$\textbackslash backslash\$nis observable in ground transportation networks, in draining and\$\textbackslash backslash\$nirrigation systems, in electric power supply systems and in natural\$\textbackslash backslash\$nobjects like the blood vessels or the trees. When alpha {$>$} 1-1/N such\$\textbackslash backslash\$nstructures can irrigate a whole bounded open set of R-N . The aim of\$\textbackslash backslash\$nthis paper is to give a mathematical proof of several structure and\$\textbackslash backslash\$nregularity properties empirically observed in transportation networks.\$\textbackslash backslash\$nIt is first proven that optimal transportation networks have a tree\$\textbackslash backslash\$nstructure and can be monotonically approximated by finite graphs. An\$\textbackslash backslash\$ninterior regularity result is then proven according to which an optimal\$\textbackslash backslash\$nnetwork is a finite graph away from the irrigated measure. It is also\$\textbackslash backslash\$nproven that the branching number of optimal networks has everywhere a\$\textbackslash backslash\$nuniversal explicit bound. These results answer questions raised in two\$\textbackslash backslash\$nrecent papers by Xia.}
}

