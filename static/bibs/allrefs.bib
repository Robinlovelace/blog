@article{molina-garcia_bicycling_2015,
  title = {Bicycling to University: Evaluation of a Bicycle-Sharing Program in {{Spain}}},
  shorttitle = {Bicycling to University},
  author = {{Molina-Garc{\'i}a}, Javier and Castillo, Isabel and Queralt, Ana and Sallis, James F.},
