@article{weiss_global_2018,
  title = {A Global Map of Travel Time to Cities to Assess Inequalities in Accessibility in 2015},
  author = {Weiss, D and Nelson, Andy and Gibson, Harry and Temperley, W and Peedell, Stephen and Lieber, A and Hancher, M and Poyart, Eduardo and Belchior, Sim{\~a}o and Fullman, Nancy and Mappin, Bonnie and Dalrymple, U and Rozier, Jennifer and Lucas, Tim and Howes, Rosalind and Tusting, Lucy and Kang, Su Yun and Cameron, E and Bisanzio, Donal and W. Gething, P},
