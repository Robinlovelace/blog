@article{bloyce_when_2018,
  title = {When Transport Policy Becomes Health Policy: {{A}} Documentary Analysis of Active Travel Policy in {{England}}},
  shorttitle = {When Transport Policy Becomes Health Policy},
  author = {Bloyce, Daniel and White, Chris},
  year = {2018},
  month = dec,
  journal = {Transport Policy},
  volume = {72},
