  language = {en},
  keywords = {completeness,data quality,open data,OpenStreetMap,routing,sidewalk},
  file = {/home/robin/Zotero/storage/4MWWXR8R/Mobasheri et al. - 2018 - Enrichment of OpenStreetMap Data Completeness with.pdf;/home/robin/Zotero/storage/XUS2SNL6/509.html}
