@book{banister_inequality_2018,
  title = {Inequality in Transport},
  author = {Banister, David},
  year = {2018},
  abstract = {Everyone needs transport to move around and to access everyday needs, but for each individual those needs are different, and they change over time and space: herein lie the seeds of inequalities in transport. In Inequality in Transport, David Banister addresses this complex problem, first through an exploration of inequality, its nature, measurement and extent. He then links inequality and the transport sector through detailed analysis of the variations in daily and long-distance travel in Great Britain over a ten-year period. He argues that there must be a much wider interpretation of inequality--one that links actual travel with measures of wellbeing and sustainability, recognizing that these will change over time. In drawing his findings together, he concludes that there must be new thinking in transport policy and planning if transport inequalities are to be alleviated.},
