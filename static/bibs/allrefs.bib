@article{johnson_extending_2015,
  title = {Extending Life on the Bike: {{Electric}} Bike Use by Older {{Australians}}},
  shorttitle = {Extending Life on the Bike},
  author = {Johnson, Marilyn and Rose, Geoff},
  year = {2015},
  month = jun,
