  keywords = {Bicycling,Bicycling: statistics \& numerical data,City Planning,City Planning: trends,Environment Design,Environment Design: trends,Forecasting,Forecasting: methods,Humans,Residence Characteristics,Social Environment,Transportation,Transportation: statistics \& numerical data,United States,Urban Health,Urban Population,Walking,Walking: statistics \& numerical data},
