@article{pucher_walking_2010,
  title = {Walking and Cycling to Health: {{A}} Comparative Analysis of City, State, and International Data},
  author = {Pucher, John and Buehler, Ralph and Bassett, David R. and Dannenberg, Andrew L.},
  year = {2010},
  journal = {American Journal of Public Health},
  volume = {100},
  number = {10},
  pages = {1986--1992},
  issn = {00900036},
  doi = {10.2105/AJPH.2009.189324},
  abstract = {OBJECTIVES: We sought to determine the magnitude, direction, and statistical significance of the relationship between active travel and rates of physical activity, obesity, and diabetes.\$\textbackslash backslash\$n\$\textbackslash backslash\$nMETHODS: We examined aggregate cross-sectional health and travel data for 14 countries, all 50 US states, and 47 of the 50 largest US cities through graphical, correlation, and bivariate regression analysis on the country, state, and city levels.\$\textbackslash backslash\$n\$\textbackslash backslash\$nRESULTS: At all 3 geographic levels, we found statistically significant negative relationships between active travel and self-reported obesity. At the state and city levels, we found statistically significant positive relationships between active travel and physical activity and statistically significant negative relationships between active travel and diabetes.\$\textbackslash backslash\$n\$\textbackslash backslash\$nCONCLUSIONS: Together with many other studies, our analysis provides evidence of the population-level health benefits of active travel. Policies on transport, land-use, and urban development should be designed to encourage walking and cycling for daily travel.},
  pmid = {20724675}
}

