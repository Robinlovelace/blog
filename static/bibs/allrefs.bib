@article{adams_prevalence_2010,
  title = {Prevalence and Socio-Demographic Correlates of ``Active Transport'' in the {{UK}}: {{Analysis}} of the {{UK}} Time Use Survey 2005},
  shorttitle = {Prevalence and Socio-Demographic Correlates of ``Active Transport'' in the {{UK}}},
  author = {Adams, Jean},
  year = {2010},
  month = apr,
  journal = {Preventive Medicine},
  volume = {50},
  number = {4},
  pages = {199--203},
  issn = {0091-7435},
  doi = {10.1016/j.ypmed.2010.01.006},
  abstract = {Objective To describe prevalence, and socio-demographic correlates, of active transport participation amongst UK adults. Methods Data was from the 2005 UK Time Use Survey. Active transport was defined as any walking, jogging or pedal biking for purposes other than enjoyment. Socio-demographic correlates of any active transport participation, meeting current UK guidelines for sufficient activity through active transport, and time spent on active transport, if any, were explored. Results 4941 individuals took part in the survey and data from 80\% were included in the analyses. 28\% of respondents reported any active transport and 19\% were sufficiently active through active transport. Median time spent in active transport, if any, was 40~min per day (inter-quartile range 20\textendash 60~min). Active transport participation was greater in younger people and those without access to a car or van. Being sufficiently active through active transport was additionally associated with being unemployed, being in a less affluent social class, and leaving full time education at an older age. Conclusions There is scope for increasing active transport participation in the UK. That more than two thirds of those who report any active transport are sufficiently active through active transport alone may be a useful health promotion message.},
  keywords = {exercise,Motor activity,Social class,Socioeconomic factors}
}

