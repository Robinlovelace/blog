@article{mindell_using_2017,
  title = {Using Triangulation to Assess a Suite of Tools to Measure Community Severance},
  author = {Mindell, Jennifer S. and Anciaes, Paulo R. and Dhanani, Ashley and Stockton, Jemima and Jones, Peter and Haklay, Muki and Groce, Nora and Scholes, Shaun and Vaughan, Laura},
