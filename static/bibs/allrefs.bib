@article{gordon-larsen_inequality_2006,
  title = {Inequality in the {{Built Environment Underlies Key Health Disparities}} in {{Physical Activity}} and {{Obesity}}},
  author = {{Gordon-Larsen}, P.},
  year = {2006},
  journal = {Pediatrics},
  volume = {117},
  number = {2},
  pages = {417--424},
  issn = {0031-4005},
  doi = {10.1542/peds.2005-0058},
  abstract = {CONTEXT Environmental factors are suggested to play a major role in physical activity (PA) and other obesity-related behaviors, yet there is no national research on the relationship between disparity in access to recreational facilities and additional impact on PA and overweight patterns in US adolescents. OBJECTIVE In a nationally representative cohort, we sought to assess the geographic and social distribution of PA facilities and how disparity in access might underlie population-level PA and overweight patterns. DESIGN, SETTING, AND PARTICIPANTS Residential locations of US adolescents in wave I (1994-1995) of the National Longitudinal Study of Adolescent Health (N = 20745) were geocoded, and a 8.05-km buffer around each residence was drawn (N = 42857 census-block groups [19\% of US block groups]). PA facilities, measured by national databases and satellite data, were linked with Geographic Information Systems technology to each respondent. Logistic-regression analyses tested the relationship of PA-related facilities with block-group socioeconomic status (SES) (at the community level) and the subsequent association of facilities with overweight and PA (at the individual level), controlling for population density. MAIN OUTCOME MEASURES Outcome measures were overweight (BMI \textbackslash textgreater or = 95th percentile of the Centers for Disease Control and Prevention/National Center for Health Statistics growth curves) and achievement of \textbackslash textgreater or = 5 bouts per week of moderate-vigorous PA. RESULTS Higher-SES block groups had a significantly greater relative odds of having 1 or more facilities. Low-SES and high-minority block groups were less likely to have facilities. Relative to zero facilities per block group, an increasing number of facilities was associated with decreased overweight and increased relative odds of achieving \textbackslash textgreater or = 5 bouts per week of moderate-vigorous PA. CONCLUSIONS Lower-SES and high-minority block groups had reduced access to facilities, which in turn was associated with decreased PA and increased overweight. Inequality in availability of PA facilities may contribute to ethnic and SES disparities in PA and overweight patterns.},
  pmid = {16452361},
  keywords = {adolescence,environment,environmental health,health disparities,population-based studies}
}

