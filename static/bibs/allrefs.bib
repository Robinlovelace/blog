@article{kamphuis_area_2008,
  title = {Area Variation in Recreational Cycling in {{Melbourne}}: A Compositional or Contextual Effect?},
  shorttitle = {Area Variation in Recreational Cycling in {{Melbourne}}},
  author = {Kamphuis, C B M and Giskes, K and Kavanagh, A M and Thornton, L E and Thomas, L R and {van Lenthe}, F J and Mackenbach, J P and Turrell, G},
