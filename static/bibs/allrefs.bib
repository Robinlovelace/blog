@article{kwan_algorithmic_2016,
  title = {Algorithmic {{Geographies}}: {{Big Data}}, {{Algorithmic Uncertainty}}, and the {{Production}} of {{Geographic Knowledge}}},
  shorttitle = {Algorithmic {{Geographies}}},
  author = {Kwan, Mei-Po},
  year = {2016},
  month = mar,
  journal = {Annals of the American Association of Geographers},
  volume = {106},
