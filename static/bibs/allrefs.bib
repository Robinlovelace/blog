  title = {Modeling of {{Influences}} of {{Vilnius}}' {{Urban Structure Development}} on {{Fuel Consumption}} in {{Private Transport System}}},
  author = {Burinskiene, M. and Klibavicius, A. and Grigonis, V. and {Uspalyte-Vitkuniene}, R.},
  year = {2009},
  month = jan,
  journal = {International Journal of Sustainable Transportation},
  volume = {3},
  number = {1},
  pages = {39--53},
  issn = {1556-8318},
  doi = {10.1080/15568310701359056},
  abstract = {The quality and supply of transport services closely relate to many environmental aspects and are among the main indicators showing the quality of life in cities. Urban sustainability is particular to each city, and this paper's research focuses on the energy consumption in Vilnius' private transport system and its dependency on land use as well as the mobility of its inhabitants. EMME/2 modeling software and the VIDAS transport database were employed to evaluate the relationship between the urban structure of Vilnius and energy consumption in private transport. The applied model was based on theoretical and practical considerations and provides clear and practical application possibilities in the analysis of different urban and transport infrastructures and their development strategies. A number of scenarios were performed in order to examine relations between land use and infrastructural changes and energy consumption. This work presents and supports the authors' hypothesis concerning the relationship between energy consumption in Vilnius' transport system and its urban structure. The research and future work based on the authors' findings for the development of Vilnius' bypasses and the reconstruction of current critical intersections showed a reduction in the current consumption of energy throughout Lithuania. However, the authors have found that in the long-term perspective, total fuel consumption will increase in the case of bypasses because longer (but faster) trips will predominate. Meanwhile, as evidence and research results shows, reconstruction of critical intersections will reduce the total fuel consumption and pollution in highly populated areas.}
}

@phdthesis{burkin_grain_2019,
  type = {M.{{A}}.},
  title = {Against the {{Grain}}: {{Safety Analysis}} and {{Design Guidance}} for {{Shared Contraflow Bike Streets}}},
  shorttitle = {Against the {{Grain}}},
  author = {Burkin, Liza P.},
  year = {2019},
  address = {{United States -- Massachusetts}},
  abstract = {This thesis explores the idea that while one-way streets are primarily meant to divert and control motor vehicle traffic, they should not limit the travel direction of bicyclists in a low-speed, low-volume context. With thoughtful design interventions, people riding bikes and driving cars can safely share space traveling in opposite directions \textendash{} even on streets that do not have space for dedicated contraflow bike lanes. An extensive European literature review, examination of six North American case studies, and video analysis from one street in Somerville, Massachusetts provide evidence that shared contraflow bike streets are a safe and useful street design for local roadways \textendash{} especially when needed for bicycle boulevards or neighborhood greenway routing. Video data collected before and after the opening of legal shared contraflow in Somerville reveal that ridership and safer positioning on the street increased, while sidewalk riding and close passing decreased. Additionally, this paper provides design guidance for different levels of shared contraflow interventions, recommends citywide contraflow policies rather than street-by-street application, and proposes public engagement and prioritization methods for studying, redesigning, and implementing shared contraflow bike streets. The project demonstrates how the absence of standards and guidance around these types of streets results in a lost opportunity for low-stress bike networks that serve people of all ages and abilities.},
  copyright = {Database copyright ProQuest LLC; ProQuest does not claim copyright in the individual underlying works.},
  isbn = {9781392546338},
  language = {English},
  school = {Tufts University},
  keywords = {bicycle,bicycle boulevard,contraflow,design guidance,one way streets,safety},
  annotation = {ZSCC: 0000001}
}

