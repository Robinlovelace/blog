@article{thornley_conspicuity_2008,
  title = {Conspicuity and Bicycle Crashes: Preliminary Findings of the {{Taupo Bicycle Study}}.},
  author = {Thornley, S J and Woodward, A and Langley, J D and Ameratunga, S N and Rodgers, a},
