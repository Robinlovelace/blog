@article{kerrjacqueline_perceived_2016,
  title = {Perceived {{Neighborhood Environmental Attributes Associated}} with {{Walking}} and {{Cycling}} for {{Transport}} among {{Adult Residents}} of 17 {{Cities}} in 12 {{Countries}}: {{The IPEN Study}}},
  shorttitle = {Perceived {{Neighborhood Environmental Attributes Associated}} with {{Walking}} and {{Cycling}} for {{Transport}} among {{Adult Residents}} of 17 {{Cities}} in 12 {{Countries}}},
  author = {{Kerr Jacqueline} and {Emond Jennifer A.} and {Badland Hannah} and {Reis Rodrigo} and {Sarmiento Olga} and {Carlson Jordan} and {Sallis James F.} and {Cerin Ester} and {Cain Kelli} and {Conway Terry} and {Schofield Grant} and {Macfarlane Duncan J.} and {Christiansen Lars B.} and {Van Dyck Delfien} and {Davey Rachel} and {Aguinaga-Ontoso Ines} and {Salvo Deborah} and {Sugiyama Takemi} and {Owen Neville} and {Mit\'a\v{s} Josef} and {Natarajan Loki}},
  year = {2016},
  month = mar,
  journal = {Environmental Health Perspectives},
  volume = {124},
  number = {3},
  pages = {290--298},
  doi = {10.1289/ehp.1409466},
  abstract = {IntroductionPrevalence of walking and cycling for transport is low and varies greatly across countries. Few studies have examined neighborhood perceptions related to walking and cycling for transport in different countries. Therefore, it is challenging to prioritize appropriate built-environment interventions.ObjectivesThe aim of this study was to examine the strength and shape of the relationship between adults' neighborhood perceptions and walking and cycling for transport across diverse environments.MethodsAs part of the International Physical activity and Environment Network (IPEN) adult project, self-reported data were taken from 13,745 adults (18\textendash 65 years) living in physically and socially diverse neighborhoods in 17 cities across 12 countries. Neighborhood perceptions were measured using the Neighborhood Environment Walkability Scale, and walking and cycling for transport were measured using the International Physical Activity Questionnaire\textendash Long Form. Generalized additive mixed models were used to model walking or cycling for transport during the last seven days with neighborhood perceptions. Interactions by city were explored.ResultsWalking-for-transport outcomes were significantly associated with perceived residential density, land use mix\textendash access, street connectivity, aesthetics, and safety. Any cycling for transport was significantly related to perceived land use mix\textendash access, street connectivity, infrastructure, aesthetics, safety, and perceived distance to destinations. Between-city differences existed for some attributes in relation to walking or cycling for transport.ConclusionsMany perceived environmental attributes supported both cycling and walking; however, highly walkable environments may not support cycling for transport. People appear to walk for transport despite safety concerns. These findings can guide the implementation of global health strategies.CitationKerr J, Emond JA, Badland H, Reis R, Sarmiento O, Carlson J, Sallis JF, Cerin E, Cain K, Conway T, Schofield G, Macfarlane DJ, Christiansen LB, Van Dyck D, Davey R, Aguinaga-Ontoso I, Salvo D, Sugiyama T, Owen N, Mit\'a\v{s} J, Natarajan L. 2016. Perceived neighborhood environmental attributes associated with walking and cycling for transport among adult residents of 17 cities in 12 countries: the IPEN study. Environ Health Perspect 124:290\textendash 298;\hspace{0.6em} http://dx.doi.org/10.1289/ehp.1409466},
  file = {/home/robin/Zotero/storage/XE4XKE6G/Kerr Jacqueline et al. - 2016 - Perceived Neighborhood Environmental Attributes As.pdf;/home/robin/Zotero/storage/TU2ECL6N/ehp.html}
}

@article{kershaw_feeling_2018,
  title = {A Feeling for Change: {{Exploring}} the Lived and Unlived Experiences of Drivers to Inform a Transition to an Electric Automobility},
  shorttitle = {A Feeling for Change},
  author = {Kershaw, Jonathan and Berkeley, Nigel and Jarvis, David and Begley, Jason},
  year = {2018},
  month = dec,
  journal = {Transportation Research Part D: Transport and Environment},
  volume = {65},
  pages = {674--686},
  issn = {1361-9209},
  doi = {10.1016/j.trd.2018.10.011},
  abstract = {This paper assesses the contemporary `consumption' of the motor-car in the context of an increased uptake of electric vehicles (EVs) as part of a transition to a low carbon automobility. Through the lens of affect and non-representational theory, it contributes to a seemingly neglected discourse by understanding how the feelings, experiences and knowledges of drivers 'conditioned' to an internal combustion engine (ICE) ecosystem might impact such a transition. This contribution is important, given that the contemporary socio-cultural significance of the car means an effective impetus for social change will come from drivers themselves. In-depth interviews explored unlived knowledges and opinions on EVs held by motorists driving ICE vehicles, comparing these with the lived experiences of EV drivers. Key findings point to levers and potential barriers. First, EV exposure suggests prospective electric propulsion carries no further fears than those experienced on any first `encounter' with a vehicle, indicating that although different, it is not viewed as a leap into the unknown. Second, ICE-drivers don't regard electric cars as inferior, providing they embody an ICE car's essence of freedom, flexibility and performance. Third, EV-drivers' sensations of avant garde and environmental-friendliness lack the physical nature of feelings expressed by ICE-drivers, yet suggest a new automobility. Addressing this `affectual' divide in how manufacturers design and disseminate knowledge about EVs is crucial to broadening their appeal. More broadly, whilst technical barriers to transition endure for consumers, the feelings and experiences concomitant with existing automobilities suggest cause for optimism regarding a potential low carbon vehicle uptake.},
  language = {en},
  keywords = {Affect,Electric cars,Knowledge and opinions,Low carbon automobility,Non-representational theory},
  file = {/home/robin/Zotero/storage/MF59BPDJ/Kershaw et al. - 2018 - A feeling for change Exploring the lived and unli.pdf;/home/robin/Zotero/storage/4WXZGCYC/S1361920917304674.html}
}

