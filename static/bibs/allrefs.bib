@article{the_roestone_collective_safe_2014,
  title = {Safe {{Space}}: {{Towards}} a {{Reconceptualization}}: {{Safe Space}}: {{Towards}} a {{Reconceptualization}}},
  shorttitle = {Safe {{Space}}},
  author = {{The Roestone Collective}},
  year = {2014},
  month = nov,
  journal = {Antipode},
  volume = {46},
  number = {5},
  pages = {1346--1365},
  issn = {00664812},
  doi = {10.1111/anti.12089},
  abstract = {The term ``safe space'' dates to the late twentieth century women's movement, but it has since been used in many different contexts. In this paper, we review and analyze historical and contemporary ``safe spaces''. These include ``separatist'' safe spaces in women's, anti-racist, and feminist communities, ``inclusive'' safe space classrooms, and safe spaces in which (non-human) objects are central. We argue that safe spaces should be understood not through static and acontextual notions of ``safe'' or ``unsafe'', but rather through the relational work of cultivating them. Such an understanding reveals several tendencies. Namely, safe spaces are inherently paradoxical. Cultivating them includes foregrounding social differences and binaries (safe\textendash unsafe, inclusive\textendash exclusive) as well as recognizing the porosity of such binaries. Renegotiating these binaries is necessarily incomplete; a safe space is never completely safe. Even so, we encourage the critical cultivation of safe space as a site for negotiating difference and challenging oppression.},
  language = {English}
}

