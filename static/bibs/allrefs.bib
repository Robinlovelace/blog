  title = {Impact of Household Proximity to the Cycling Network on Bicycle Ridership: {{The}} Case of {{Bogot\'a}}},
  shorttitle = {Impact of Household Proximity to the Cycling Network on Bicycle Ridership},
  author = {{Rodriguez-Valencia}, Alvaro and {Rosas-Satiz{\'a}bal}, Daniel and Gordo, Daniel and Ochoa, Andr{\'e}s},
  year = {2019},
