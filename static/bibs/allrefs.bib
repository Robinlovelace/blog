@article{appleyard_meaning_2017,
  title = {The Meaning of Livable Streets to Schoolchildren: {{An}} Image Mapping Study of the Effects of Traffic on Children's Cognitive Development of Spatial Knowledge},
  shorttitle = {The Meaning of Livable Streets to Schoolchildren},
  author = {Appleyard, Bruce},
