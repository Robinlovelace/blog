@article{dejesus_influence_2021,
  title = {Influence of Active Commuting to School on Daily Physical Activity among Children and Adolescents},
  author = {{de Jesus}, Gilmar Merc{\^e}s and {Henrique de Oliveira Araujo}, Raphael and Dias, Lizziane Andrade and Cerqueira Barros, Anna Karolina and {Matos dos Santos Araujo}, Lara Daniele and {Altenburg de Assis}, Maria Alice},
  year = {2021},
