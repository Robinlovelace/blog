  keywords = {Acceptance,Bicycle facilities,Bicycle travel,Bicycling,Bogota (Colombia),Built environment,commuting,Demographics,gender,Physical Fitness,Public Health,Qualitative analysis,Qualitative assessment,Social factors}
}

