@article{marrero_convergence_2021,
  title = {Convergence in Road Transport {{CO2}} Emissions in {{Europe}}},
  author = {Marrero, {\'A}ngel S. and Marrero, Gustavo A. and Gonz{\'a}lez, Rosa Marina and {Rodr{\'i}guez-L{\'o}pez}, Jes{\'u}s},
  year = {2021},
  month = jul,
  journal = {Energy Economics},
  volume = {99},
  pages = {105322},
  issn = {0140-9883},
  doi = {10.1016/j.eneco.2021.105322},
  abstract = {In Europe, the transport sector accounts for more than 27\% of total CO2 emissions and, within this sector, road transport is by far the largest polluter. This fact has placed road transport emissions abatement firmly on the agenda of global alliances. In this paper, we examine the convergence in per capita road transport CO2 emissions in a sample of 22 European countries over the 1990\textendash 2014 period. We find evidence that European countries converge to one another but depending on certain structural factors (conditional convergence), and that the convergence speed has increased over time. Our results show that this conditional convergence process has led to undesirably high levels of emissions since a large part of the laggard (less polluting) countries have caught up with the leaders (more polluting) countries. We then estimate a conditional convergence dynamic panel data model to examine the structural factors affecting the convergence process and its influence on the convergence speed. Given that in our sample road transport CO2 emissions depend almost exclusively on (fossil) fuel consumption, we focus on the determinants channelled through the use of energy in the sector. By using alternative econometric approaches (pooled-OLS, fixed-effects and instrumental variables), our results show that the convergence process is conditioned by factors such as economic activity and fuel prices, and that some of these factors have a significant effect on the convergence speed. Our results imply that the European transport policy should reduce the differences in structural factor endowments among countries to enable the absolute convergence towards a lower level of emissions in a reasonable period of time.},
  language = {en},
  keywords = {CO emissions,Convergence,Dynamic panel data models,Road transport}
}

